{
  "name": "landing-page",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.11.1",
    "pnpm": ">=8.15.8"
  },
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch src/scss:src/css",
    "server": "http-server src -p 3000 -c-1 --cors",
    "start": "npm-run-all --parallel watch:sass server",
    "build": "sass src/scss:dist/css --no-source-map --style=compressed",
    "lint": "eslint src --ext .js",
    "lintfix": "eslint src --ext .js --fix",
    "format": "prettier \"**/*.{js,json,html}\" --write",
    "formatcheck": "prettier \"**/*.{js,json,html}\" --check"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.1",
    "sass": "^1.81.0"
  }
}
